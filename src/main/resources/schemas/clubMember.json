{
  "type" : "object",
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "description" : "A club member",
  "title" : "Club Member",
  "properties" : {
    "id" : {
      "type" : "integer"
    },
    "lastName" : {
      "type" : "string",
      "minimum":1,
      "maximum": 255
    },
    "name" : {
      "type" : ["string"],
      "minLength": 3,
      "maxLength": 255
    },
    "memberNumber" : {
      "type" : "string",
      "minLength": 3,
      "maxLength": 255
    }
  },
  "required":["name","lastName","memberNumber"]
}
